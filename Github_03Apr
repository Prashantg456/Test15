package Arrays;

public class Char1 
{
	public static void main (String[]args)
	{
		   // array declaration
		  char ar2[]=new char[4];
		
		  // array initialization
		    ar2[0]='A';
		    ar2[1]='B';
		    ar2[2]='C';
		    ar2[3]='D';

		  // usage 
		     System.out.println(ar2[0]);
			 System.out.println(ar2[1]);
			 System.out.println(ar2[2]);
			 System.out.println(ar2[3]);
		    
}
}
